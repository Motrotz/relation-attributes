{
    "name": "motrotz/relation-attributes",
    "description": "Define Eloquent relationships using Attributes.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RelationAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.0"
    },
    "scripts": {
        "test": "pest",
        "lint": "pint"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
